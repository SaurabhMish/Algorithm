#include<iostream>

using namespace std;

int search(int arr[],int n,int x){
    for(int i=0;i<n;i++){
        if(arr[i]==x){
            return i;
        }
        else{
            return -1;
        }
    }

}
int main(void){

    int arr[] ={10,20,30,40,50,110,120,90,36};
    int x=40;
    int n = sizeof(arr)/sizeof(arr[0]);
    int result = search(arr,n,x);

    return 0;
}
